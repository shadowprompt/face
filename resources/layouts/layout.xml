<layout id="WatchFace">
    <drawable id="background"></drawable>
    <!-- 电量 -->
    <label id="batteryLabel" x="140" y="0" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_LT_GRAY" />
    <!-- 天气 -->
    <label id="weatherLabel" x="90" y="60" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_LT_GRAY" />
    <!-- 步数 -->
    <label id="stepText" x="195" y="70" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
    <!-- 心率 -->
    <label id="heartrateText" x="115" y="160" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
    <!-- 最大摄氧量 -->
    <label id="vo2maxText" x="180" y="160" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
    <!-- 距离 -->
    <label id="distText" x="210" y="160" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
    <!-- 卡路里 -->
    <label id="caloryText" x="55" y="160" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
    <!-- 身体电量 -->
    <label id="bodyBatteryText" x="85" y="160" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
    <!-- 消息数 -->
    <label id="msgText" x="150" y="160" font="Graphics.FONT_XTINY" justification="Graphics.TEXT_JUSTIFY_CENTER" color="Graphics.COLOR_WHITE" />
    <!-- <drawable id="Smiley2"></drawable> -->
    <!-- <drawable id="CustomMoveBar"></drawable> -->
